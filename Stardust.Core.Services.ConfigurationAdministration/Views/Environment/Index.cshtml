@model IEnumerable<Stardust.Core.Services.ConfigurationReader.EnvironmentConfig>

@{
    ViewBag.Title = "View1";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View1</h2>

<p>
    @Html.ActionLink("Create New", "CreateEnvironment",new {id=ViewBag.SetName})
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EnvironmentName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EnvironmentName)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditEnvironment", new {id=ViewBag.SetName, name=item.EnvironmentName}) 
        </td>
    </tr>
}

</table>
