<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="moduleCreator" type="Stardust.Nucleus.Configuration.ModuleCreatorConfigurationSettings, Stardust" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <moduleCreator>
    <moduleCreators>
      <module baseModuleName="Stardust.Particles.ILogging">
        <implementations>
          <implementationReference implementationKey="default" type="Stardust.Particles.LoggingDefaultImplementation, Stardust" />
        </implementations>
      </module>
      <module baseModuleName="Stardust.Core.CrossCuttingTest.LegacyTests.IConfiguredInstance">
        <implementations>
          <implementationReference implementationKey="default" type="Stardust.Core.CrossCuttingTest.LegacyTests.DefaultInstance, Stardust.Core.CrossCuttingTest, Version=1.0.0.0, Culture=neutral" scope="Singleton" />
          <implementationReference implementationKey="fromConfig" type="Stardust.Core.CrossCuttingTest.LegacyTests.AlternateInstance, Stardust.Core.CrossCuttingTest, Version=1.0.0.0, Culture=neutral" />
        </implementations>
      </module>
    </moduleCreators>
  </moduleCreator>
  <appSettings>
    <add key="log4net.Internal.Quiet" value="true" />
    <add key="IncludePrivates" value="true" />
    <add key="serviceName" value="Test" />
    <add key="configSet" value="v1-UnitTest" />
    <add key="stardust.UseTransientKernel" value="true" />
  </appSettings>
  <log4net>
    <appender name="StardustLoggingAppender" type="log4net.Appender.RollingFileAppender">
      <appendToFile value="false" />
      <param name="LockingModel" type="log4net.Appender.FileAppender+MinimalLock" />
      <rollingStyle value="Composite" />
      <datePattern value=".yyyyMMdd-HH" />
      <maxSizeRollBackups value="10" />
      <file type="log4net.Util.PatternString" value="C:\temp\logging\log.txt" />
      <maximumFileSize value="50B" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d %-5p %c - %m%n" />
      </layout>
    </appender>
    <logger name="StardustLogging">
      <maximumFileSize value="2MB" />
      <appender-ref ref="StardustLoggingAppender" />
    </logger>
  </log4net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /></startup>

  <runtime>

    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">

      <dependentAssembly>

        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />

      </dependentAssembly>

    </assemblyBinding>

  </runtime>

</configuration>
