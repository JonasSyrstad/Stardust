@model IEnumerable<Stardust.Starterkit.Configuration.Repository.IEndpoint>

<p>
    @Html.ActionLink("Create New", "Create","Endpoint",new {id="new", command=ViewBag.ServiceId},null)
</p>
<table class="table table-striped table-hover table-condensed ">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in from m in Model orderby m.Name ascending select m)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", "Endpoint", new { id = "edit", item = item.Id }, null) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
